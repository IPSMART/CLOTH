            font-family: Arial,"Helvetica Neue",Helvetica,sans-serif;
            font-size: 11px;
            line-height: 1.4;
            color: #242729;
            text-shadow: 0 1px 0 #FFF;
            background-color: #e1e3e5;
            border: 1px solid #adb3b9;
            border-radius: 3px;
            box-shadow: 0 1px 0 rgba(12,13,14,0.2), 0 0 0 2px #FFF inset;
            white-space: nowrap;
        }

        .t_gradient {
            margin-top: 20px;
            background-image: linear-gradient(90deg, #7ee289, #FFFFFF);
        }

        .m_gradient {
            margin-top: 17px;
            background-image: linear-gradient(90deg, #CDDC39, #f7ffff);
        }
    </style>

    <script>
        function saveQues() {
            $.ajax({
                type: 'post',
                url: 'SaveData',
                data: $('form').serialize(),
                success: function (result) {
                    alert(result);
                },
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    msgError("Error: " + textStatus + "," + errorThrown);
                }
            });
        }
        function addRowUsr() {
            $.ajax({
                type: 'post',
                url: 'AddRowUG',
                data: $('form').serialize(),
                success: function (result) {
                    $("#partialdivUG").html(result);
                }
            });
        }
        function deleteRowUsr() {
            $.ajax({
                type: 'post',
                url: 'DeleteRowUG',
                data: $('form').serialize(),
                success: function (result) {
                    $("#partialdivUG").html(result);
                }
            });
        }
    </script>
</head>
