    {
        if ("@Model.DefaultAction" != "V")
        {
            var ID_VAL = document.getElementById(ITCD).value;
            if (id == "") { id = ID_VAL; TAG = "C"; }
            if (id == "")
            {
                ClearAllTextBoxes((STYLENO + "," + ITCD + "," + ITNM + "," + UOMNM + "," + PCSPERBOX));
            }
            else
            {
                $.ajax({
                    type: 'GET',
                    url: "@Url.Action("GetArticleItemDetails", PageControllerName)",
