                            $("#Msgdiv1").show();
                            $("#info").show();
                            ClearAllTextBoxes((STYLENO + "," + ITCD + "," + ITNM + "," + UOMNM + "," + PCSPERBOX));
                            $("#btnok").focus();
                            $("#msgbody_info").html(result);
                            message_value = STYLENO;
