<script>
    function OpenSize(ID, i, URLNAME)
    {
        const keyName = event.key;
        if (keyName == undefined || keyName == " ")
        {
            var SerialNo = document.getElementById(ID + "SLNO_" + i).value;
            if (SerialNo == "") { SerialNo = parseInt(0) } else { SerialNo = parseInt(SerialNo) }
            var ITEM = document.getElementById(ID + "ITCD_" + i).value;
            var ITEM_NM = document.getElementById(ID + "ITNM_" + i).value;
            var STYLE_NO = document.getElementById(ID + "STYLENO_" + i).value; STYLE_NO = STYLE_NO.replace('+', 'μ'); STYLE_NO = STYLE_NO.replace('&', '‡');
            var UOM = document.getElementById(ID + "UOMNM_" + i).value;
            var PART_CD = document.getElementById(ID + "PARTCDM_" + i).value;
            var PART_NM = document.getElementById(ID + "PARTNMM_" + i).value;
            var STK_TYPE = document.getElementById(ID + "STKTYPE_" + i).value;
            var PCS_PERSET = document.getElementById(ID + "PCSPERSET_" + i).value;
            if (PCS_PERSET == "") { PCS_PERSET = parseFloat(0) } else { PCS_PERSET = parseFloat(PCS_PERSET) }
            var PCSPERBOX = document.getElementById(ID + "PCSBOX_" + i).value;
            if (PCSPERBOX == "") { PCSPERBOX = parseFloat(0) } else { PCSPERBOX = parseFloat(PCSPERBOX) }
            var STK_TYPE = document.getElementById(ID + "STKTYPE_" + i).value;
            $("#hiddenITEM").val(ITEM);
            if (ITEM.toString() != "")
            {
                $.ajax({
                    type: 'POST',
                    url: "../@PageControllerName/" + URLNAME,
                    beforesend: $("#WaitingMode").show(),
                    data: $('form').serialize() + "&SerialNo=" + SerialNo + "&ITEM=" + ITEM + "&ITEM_NM=" + ITEM_NM + "&STYLE_NO=" + STYLE_NO + "&UOM=" + UOM + "&PART_CD=" + PART_CD + "&PART_NM=" + PART_NM + "&PCSPERBOX=" + PCSPERBOX + "&STK_TYPE=" + STK_TYPE + "&PCS_PERSET=" + PCS_PERSET + "&TAG=" + ID,
                    success: function (result)
                    {
                        $("#WaitingMode").hide();                      
                        $('#popup').html(result);
                        $('#popup').show();
                        $('.PopUp_Backround').show();
                    },
                    error: function (XMLHttpRequest, textStatus, errorThrown)
                    {
                        $("#WaitingMode").hide();
                        msgError("Error: " + textStatus + "," + errorThrown);
                    }
                });
            }
            else
            {
                $("#Msgdiv1").show();
                $("#info").show();
                $("#btnok").focus();
                $("#msgbody_info").html("Article Number is not Available ! Please Select / Enter a Article Number !! ");
                message_value = "OUT_ITCD_" + i;
            }
        }
        else
        {
            preventEvent(event);
        }
    }

    document.onkeydown = CloseSize;
    function CloseSize(TAG, TABLE, URLNAME)
    {
        var KeyID = (window.event) ? event.keyCode : e.keyCode;
        if (KeyID == undefined || KeyID == 27)
        {
            if ("@Model.DefaultAction" != "V")
            {
                $.ajax({
                    type: 'POST',
                    url: "../@PageControllerName/" + URLNAME,
                    beforesend: $("#WaitingMode").show(),
                    data: $('form').serialize(),
                    success: function (result)
                    {
                        $("#WaitingMode").hide();
                        if (TAG == "IN_")
                        {
                            $('#partialdivIN').html(result);
                        }
                        else
                        {
                            $('#partialdivOUT').html(result);
                        }
                        $("#popup").html("");
                        $('.PopUp_Backround').fadeOut();
                        var GridRowMain = $("#" + TABLE + " > tbody > tr").length;
                        for (i = 0; i <= GridRowMain - 1; i++)
                        {
                            var ARTICLE = document.getElementById(TAG + "STYLENO_" + i).value;
                            if (ARTICLE == "")
                            {
                                $(TAG + "#STYLENO_" + i).focus();
                                break;
                            }
                        }
                        CalculateTotal(TAG, TABLE);
                    },
                    error: function (XMLHttpRequest, textStatus, errorThrown)
                    {
                        $("#WaitingMode").hide();
                        msgError("Error: " + textStatus + "," + errorThrown);
                    }
                });
            }
        }
    }
    function CancelSize()
    {        
        $("#popup").html("");
        $('.PopUp_Backround').fadeOut();
    }
    function CalculateSizeData(id, QNTY, SLNO, ITCD, ITNM, PCSPERSET, ARTNO, UOM, TOTAL_PCS, STKTYPE, FREESTK, RT)
    {
        if ("@Model.DefaultAction" != "V")
        {
            var ARTICLE = document.getElementById(ARTNO).value;
            if (ARTICLE != "")
            {
                var NO_OF_SETS = id;
                if (NO_OF_SETS == "") { NO_OF_SETS = parseFloat(0); } else { NO_OF_SETS = parseFloat(NO_OF_SETS); }
                if (NO_OF_SETS > 0)
                {
                    OpenSize(QNTY, SLNO, ITCD, ITNM, PCSPERSET, ARTNO, UOM, TOTAL_PCS, NO_OF_SETS, STKTYPE, FREESTK, RT, 0);
                }
            }
            else
            {
                $("#Msgdiv1").show();
                $("#info").show();
                $("#msg_ok_info").focus();
                $("#msgbody_info").html("Aricle Number not Available ! Please Select / Enter a Valid Article Number !! ");
                message_value = ARTNO.id;
                return false;
            }
        }
    }

    function CalculateSizeQnty(QNTY, TABLE, ID)
    {
        var CHECK_TAG = document.getElementById(ID + "SIZE_SAME_QTY").checked;
        if (CHECK_TAG == true)
        {
            var GridRow = $("#" + TABLE + " > tbody > tr").length;
            for (var i = 0; i <= GridRow - 1; i++)
            {
                var QUANTITY = document.getElementById(ID + "SIZE_QNTY_" + i).value;
                if (QUANTITY == "") { QUANTITY = parseFloat(0) } else { QUANTITY = parseFloat(QUANTITY) }
                if (QUANTITY == 0)
                {
                    document.getElementById(ID + "SIZE_QNTY_" + i).value = QNTY;
                }
            }
        }
    }
</script>
