<script>
    function CalculateTotal(ID, TABLE)
    {
        var T_QNTY = 0; var T_BOX = 0; var T_SET = 0; var T_FQNTY = 0; var T_RQNTY = 0; var T_LQNTY = 0; var T_DQNTY = 0;
        var GridRow = $("#" + TABLE + " > tbody > tr").length;
        for (var i = 0; i <= GridRow - 1; i++)
        {
            var QNTY = document.getElementById(ID + "QNTY_" + i).value; if (QNTY != "") { T_QNTY = T_QNTY + parseFloat(QNTY); } else { T_QNTY = T_QNTY + parseFloat(0); }
            var BOX = document.getElementById(ID + "BOXES_" + i).value; if (BOX != "") { T_BOX = T_BOX + parseFloat(BOX); } else { T_BOX = T_BOX + parseFloat(0); }
            var SET = document.getElementById(ID + "SETS_" + i).value; if (SET != "") { T_SET = T_SET + parseFloat(SET); } else { T_SET = T_SET + parseFloat(0); }

            var STK_TYPE = document.getElementById(ID + "STKTYPE_" + i).value;
            if (STK_TYPE == "F")
            {
                if (QNTY != "") { T_FQNTY = T_FQNTY + parseFloat(QNTY); } else { T_FQNTY = T_FQNTY + parseFloat(0); }                
            }
            else if (STK_TYPE == "R")
            {
                if (QNTY != "") { T_RQNTY = T_RQNTY + parseFloat(QNTY); } else { T_RQNTY = T_RQNTY + parseFloat(0); }
            }
            else if (STK_TYPE == "L")
            {
                if (QNTY != "") { T_LQNTY = T_LQNTY + parseFloat(QNTY); } else { T_LQNTY = T_LQNTY + parseFloat(0); }
            }
            else if (STK_TYPE == "D")
            {
                if (QNTY != "") { T_DQNTY = T_DQNTY + parseFloat(QNTY); } else { T_DQNTY = T_DQNTY + parseFloat(0); }
            }
        }
        if (ID == "IN_")
        {
            document.getElementById("S_T_IN_QNTY_0").value = parseFloat(T_FQNTY).toFixed(3);
            document.getElementById("S_T_IN_QNTY_1").value = parseFloat(T_RQNTY).toFixed(3);
            document.getElementById("S_T_IN_QNTY_2").value = parseFloat(T_LQNTY).toFixed(3);
            document.getElementById("S_T_IN_QNTY_3").value = parseFloat(T_DQNTY).toFixed(3);
        }
        else
        {
            document.getElementById("S_T_OUT_QNTY_0").value = parseFloat(T_FQNTY).toFixed(3);
            document.getElementById("S_T_OUT_QNTY_1").value = parseFloat(T_RQNTY).toFixed(3);
            document.getElementById("S_T_OUT_QNTY_2").value = parseFloat(T_LQNTY).toFixed(3);
            document.getElementById("S_T_OUT_QNTY_3").value = parseFloat(T_DQNTY).toFixed(3);
        }

        var GridRowSTOCK = $("#_T_StockAdj_STK_TYPE_QNTY_GRID > tbody > tr").length;
        var TOTAL_IN = 0; var TOTAL_OUT = 0;
        for (var i = 0; i <= GridRowSTOCK - 1; i++)
        {
            var IN_QNTY = document.getElementById("S_T_IN_QNTY_" + i).value; if (IN_QNTY != "") { TOTAL_IN = TOTAL_IN + parseFloat(IN_QNTY); } else { TOTAL_IN = TOTAL_IN + parseFloat(0); }
            var OUT_QNTY = document.getElementById("S_T_OUT_QNTY_" + i).value; if (OUT_QNTY != "") { TOTAL_OUT = TOTAL_OUT + parseFloat(OUT_QNTY); } else { TOTAL_OUT = TOTAL_OUT + parseFloat(0); }
        }
        document.getElementById(ID + "T_QNTY").value = parseFloat(T_QNTY).toFixed(3);
        document.getElementById(ID + "TOTAL_BOXES").value = T_BOX;
        document.getElementById(ID + "TOTAL_SETS").value = T_SET;
        document.getElementById("TOTAL_IN_QNTY").value = parseFloat(TOTAL_IN).toFixed(3);
        document.getElementById("TOTAL_OUT_QNTY").value = parseFloat(TOTAL_OUT).toFixed(3);
    }
</script>

<script>
    function SelectMtrlJobCode(id, MTRLJOBCD, MTRLJOBNM) {
        if (id == "") {
            $("#" + MTRLJOBCD.id).val("");
            $("#" + MTRLJOBNM.id).val("");
        }
        else {
            $.ajax({
                type: 'GET',
                url: '@Url.Action("GetMtrlJobDetails", PageControllerName)',
                data: { val: id },
                success: function (result) {
                    var MSG = result.indexOf(String.fromCharCode(181));
                    if (MSG >= 0) {
                        $("#tempHDD").val(result);
                        var findstr = $("#tempHDD").val();
                        document.getElementById(MTRLJOBCD.id).value = returncolvalue(findstr, "MTRLJOBCD");
                        document.getElementById(MTRLJOBNM.id).value = returncolvalue(findstr, "MTRLJOBNM");
                    }
                    else {
                        $("#Msgdiv1").show();
                        $("#info").show();
                        $("#" + MTRLJOBCD.id).val("");
                        $("#" + MTRLJOBNM.id).val("");
                        $("#btnok").focus();
                        $("#msgbody_info").html(result);
                        message_value = MTRLJOBCD.id;
                    }
                },
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    $("#WaitingMode").hide();
                    msgError(XMLHttpRequest.responseText);
                    $("body span h1").remove(); $("#msgbody_error style").remove();
                }
            });
        }
    }
</script>

