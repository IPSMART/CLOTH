<script>
    function GetPendingData() {
        debugger;
        if ("@Model.DefaultAction" != "V") {
            if (!emptyFieldCheck("Please Select / Enter Document Date", "DOCDT")) { return false; }
            var mutslcd = "";
            var docdt = $("#DOCDT").val();
            if ($("#MUTSLCD").val() != "") mutslcd = $("#MUTSLCD").val();

            $.ajax({
                type: 'POST',
                url: "@Url.Action("GetPendingData", PageControllerName)",
                data: "&DOCDT=" + docdt + "&MUTSLCD=" + mutslcd,
                success: function (result) {
                    if (result == 0)
                    { return msgWarning("Record not Found! Please enter valid Document date."); }
                    $("#popup").animate({ marginTop: '-10px' }, 50);
                    $("#popup").html(result);
                },
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    $("#WaitingMode").hide();
                    msgError(XMLHttpRequest.responseText);
                    $("body span h1").remove(); $("#msgbody_error style").remove();
                }
            });
        }
    }

</script>
<script>
    function SelectMatchingCheckBoxes() {
        debugger;;
        var arr = new Array();
        var GridRowMain = $("#T_Bilty_Khasra_PopUp > tbody > tr").length;
        for (i = 0; i <= GridRowMain - 1; i++) {
            var chkbox = document.getElementById("chkid_K_POPUP_" + i);
            if (chkbox.checked) { arr.push(document.getElementById("BLAUTONO_K_POPUP_" + i)); }

        }
        for (i = 0; i <= GridRowMain - 1; i++) {
            if (arr.indexOf(document.getElementById("BLAUTONO_K_POPUP_" + i)) != -1) {
                document.getElementById("chkid_K_POPUP_" + i).checked = true;
            }
        }

    }
    function SelectPendingLRNO() {
        debugger;
        var mutslcd = "";
        var docdt = $("#DOCDT").val();
        if ($("#MUTSLCD").val() != "") mutslcd = $("#MUTSLCD").val();
        var Count = 0;
        var GridPendingPO = $("#T_Bilty_Khasra_PopUp > tbody > tr").length;
        for (var i = 0; i <= GridPendingPO - 1; i++) {
            var Check = document.getElementById("chkid_K_POPUP_" + i).checked;
            if (Check == true) {
                Count = Count + 1;
            }
        }
        if (Count > 0) {
            $.ajax({
                type: 'post',
                url: "@Url.Action("SelectPendingLRNO", PageControllerName )",
                beforesend: $("#WaitingMode").show(),
                data: $('form').serialize() + "&DOCDT=" + docdt + "&MUTSLCD=" + mutslcd,
                success: function (result) {
                    //var res = result.split("^^^^^^^^^^^^~~~~~~^^^^^^^^^^");

                    $("#partialdivMain").animate({ marginTop: '0px' }, 50);
                    $("#partialdivMain").html(result);
                    $("#WaitingMode").hide();
                    @*$('#partialdivMain').html(res[0]);
                    if ("@Session["CLIENT_CODE"]" == "ADHU") {
                        $("#OTHREFNO").val(res[1]);
                    }*@
                    $('#popup').html("");
                    $('.PopUp_Backround').fadeOut();
                },
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    $("#WaitingMode").hide();
                    msgError(XMLHttpRequest.responseText);
                    $("body span h1").remove(); $("#msgbody_error style").remove();
                }
            });
        }

    }

    function CancelLRNO() {
        $('#popup').html("");
        $('.PopUp_Backround').fadeOut();
    }
</script>
<script>
    function changeSTCHNM(id) {
        debugger;
        var value = ($("#" + id).find(":selected").val());
        $.ajax({
            type: 'GET',
            url: "@Url.Action("changeSTCHNM", PageControllerName)",
            data: {
                val: value
            },
            success: function (result) {
                debugger;
                $("#WaitingMode").hide();
                //var obj = JSON.parse(result);
                if (result.message == "ok") {
                    document.getElementById("BuyerGstin").value = result.BuyerGstin;
                    document.getElementById("DocDt").value = result.DocDt;
                    document.getElementById("DocNo").value = result.DocNo;
                    document.getElementById("DocTyp").value = result.DocTyp;
                    document.getElementById("Irn").value = result.Irn;
                    document.getElementById("IrnDt").value = result.IrnDt;
                    document.getElementById("ItemCnt").value = result.ItemCnt;
                    document.getElementById("MainHsnCode").value = result.MainHsnCode;
                    document.getElementById("SellerGstin").value = result.SellerGstin;
                    document.getElementById("TotInvVal").value = result.TotInvVal;
                } else {
                    msgInfo(result.message);
                }


                debugger
                if (result == "0") {
                } else {//

                    document.getElementById("FLDCD").value = returncolvalue(result, "FLDCD");
                    var chkfldnm = returncolvalue(result, "FLDNM");
                    if (chkfldnm == "")
                    { document.getElementById("FLDDESC").value = returncolvalue(result, "FLDDESC"); }
                    else { document.getElementById("FLDDESC").value = returncolvalue(result, "FLDNM"); }
                    document.getElementById("FLDDATACOMBO").value = returncolvalue(result, "FLDDATACOMBO");
                    document.getElementById("FLDLEN").value = returncolvalue(result, "FLDLEN");
                    document.getElementById("FLDTYPE").value = returncolvalue(result, "FLDTYPE");
                }
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                $("#WaitingMode").hide();
                msgError(XMLHttpRequest.responseText);
                $("body span h1").remove(); $("#msgbody_error style").remove();
            }
        });
    }
</script>

