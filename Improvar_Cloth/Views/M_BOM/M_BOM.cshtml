<script language="javascript">
    document.onkeyup = KeyCheck;
    function KeyCheck(e) {
        var KeyID = (window.event) ? event.keyCode : e.keyCode;
        if (KeyID == 118) {
            var GridRowOperation = $("#M_BOM_OPERATION > tbody > tr").length;
            var LST_PART_CD = "";
            var LST_PART_NM = "";
            var LST_SEQ_ORD = "";
            for (i = 0; i <= GridRowOperation - 1; i++) {
                var PART_CD = document.getElementById("PARTCDO_" + i).value;
                var PART_NAME = document.getElementById("PARTNMO_" + i).value;
                var SEQ_ORD = document.getElementById("SEQORDNOO_" + i).value;
                if (PART_CD != "") {
                    LST_PART_CD = PART_CD;
                    LST_PART_NM = PART_NAME;
                    if (SEQ_ORD != "") {
                        LST_SEQ_ORD = SEQ_ORD;
                    }
                }

                if (document.getElementById("PARTCDO_" + i).value == "") {
                    document.getElementById("PARTCDO_" + i).value = LST_PART_CD;
                    document.getElementById("PARTNMO_" + i).value = LST_PART_NM;
                    var SEQ = parseInt(LST_SEQ_ORD) + 1;
                    var pad = "00"
                    var ans = pad.substring(0, pad.length - SEQ.toString().length) + SEQ
                    document.getElementById("SEQORDNOO_" + i).value = ans;
                    return false;
                }
            }
        }
    }
</script>
