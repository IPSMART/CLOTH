            $("#" + JOBNM_.id).val("");
        }
        else {
            $.ajax({
                type: 'GET',
                url: $("#urlnameSUBJOB").val(),
                data: {
                    val: id,
                    val1: EFF_DT
                },
                success: function (result) {
                    if (result == "0") {
                        $("#Msgdiv1").show();
