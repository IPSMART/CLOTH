                                <input type="button" class="btn-sm btn-info" onclick="OS_RM_MailSend('@index');" name="Email" value="Email" />
                                <input type="button" class="btn-sm btn-info" onclick="OS_RM_SMSSend('@index');" name="SMS" value="SMS" />
                                <img src="~/Image/squere.png" style="float:right;padding-top:3px;padding-right:25px" width="62px" height="40px" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
