                @Html.HiddenFor(model => model.DashboardList[index].Permission, new { id = "FABOARD3_Permission" })
                <div id="@Model.DashboardList[index].BoardCode">
                    <div class="col-md-9">
                        <div style="height:250px;border:4px solid #86abd8;border-radius:15px;position:relative;width:367px;margin-top:10px;margin-left:17px">
                            <div class="row">
                                <div class="col-sm-6 tabtitle" style="padding-top:5px;">
                                    @Html.DisplayFor(model => model.DashboardList[index].Caption)
                                </div>
                                <div class="col-sm-3" style="padding-top:2px;"><span title="Last Updated Time" id="span_authenticationDash">@Html.DisplayFor(model => model.DashboardList[index].RefreshedTime)</span> </div>
                                <div class="col-sm-1">
                                    <span title="Refresh Board" class="glyphicon glyphicon-refresh" style="color:greenyellow;vertical-align:middle;cursor:pointer;"
                                          onclick="UpdateDashboard('@Model.DashboardList[index].BoardCode','@index')"></span>
                                </div>
                            </div>
                            <div class="row">
                                <div style="padding:0px 0px;background-color:#ffffff;margin-top:10px;height:159px;overflow-y:auto">
