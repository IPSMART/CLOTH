                                        <tr>
                                            <td>
                                                @Html.Label("", "Report On")
                                            </td>
                                            <td>
                                                <label class="radio-inline">
                                                    @Html.RadioButtonFor(model => model.DashboardList[index].SPREPORT.salepur, "P", new { id = "salepur", onchange = "UpdateLedger(" + index + ")" })  @Html.Label("Purchase")
                                                </label>
                                                <label class="radio-inline">
                                                    @Html.RadioButtonFor(model => model.DashboardList[index].SPREPORT.salepur, "S", new { id = "salepur" , onchange = "UpdateLedger(" + index + ")" })  @Html.Label("Sales")
                                                </label>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                @Html.Label("", "From Date")
                                            </td>
                                            <td style="width:113px;">
                                                @Html.EditorFor(model => model.DashboardList[index].SPREPORT.fdt, new { htmlAttributes = new { @class = "form-control", id = "fdt", Style = "height: 25px;", onblur = "DocumentDateCHK(this);", @placeholder = "dd/mm/yyyy", autocomplete = "off", maxlength = 10 } })
                                                <script>
                                                    $(function () {
                                                        $("#fdt").datepicker({ dateFormat: "dd/mm/yy", changeMonth: true, changeYear: true });
                                                    });
                                                </script>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                @Html.Label("", "To Date")
                                            </td>
                                            <td style="width:113px;">
                                                @Html.EditorFor(model => model.DashboardList[index].SPREPORT.tdt, new { htmlAttributes = new { @class = "form-control", id = "tdt", Style = "height: 25px;", onblur = "DocumentDateCHK(this);", @placeholder = "dd/mm/yyyy", autocomplete = "off", maxlength = 10 } })
                                                <script>
                                                    $(function () {
                                                        $("#tdt").datepicker({ dateFormat: "dd/mm/yy", changeMonth: true, changeYear: true });
                                                    });
                                                </script>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                @Html.Label("", "Ledger")
                                            </td>
                                            <td style="width:113px;">
                                                <div id="partialdivLedger">
                                                    @(new HtmlString(Model.DashboardList[index].SPREPORT.glcd))
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                @Html.Label("", "No. of Record Showing(Top)")
                                            </td>
                                            <td style="width:113px;">
                                                @Html.EditorFor(model => model.DashboardList[index].SPREPORT.noofrec, new { htmlAttributes = new { @class = "form-control", id = "noofrec", @onkeypress = "return numericOnly(this,0);", maxlength = 3, Style = "height: 25px;", autocomplete = "off" } })
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                            </div>
                            <div style="background:linear-gradient(#eeeeee,#dbd7d7);height:42px;width:100%; position:absolute;bottom:0;left:0; border-bottom-left-radius:10px;border-bottom-right-radius:10px">
                                @*<input type="button" class="btn-sm btn-info" onclick="DownloadExcelSalePur('@index');" name="Excel" value="Excel" />*@
                                <button class="btn-sm btn-info" formtarget="_blank" name="btnPostAction" onclick="FormSubmit('@Model.DashboardList[index].BoardCode','@index')" style="margin-top: 5px;">Excel</button>

                                <img src="~/Image/squere.png" style="float:right;padding-top:3px;padding-right:25px" width="62px" height="40px" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        }
    }
