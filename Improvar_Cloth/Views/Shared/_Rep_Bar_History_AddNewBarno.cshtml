@model Improvar.ViewModels.RepBarHistory
<div id="dragclass" style="overflow-y:auto;max-height:500px;height:auto;overflow-x:hidden;background-color:#c4b9b9;padding:7px;border-radius: 7px;transform: translate(0%, 0%);z-index: 1100;width:500px;">
    <div class="table-responsive">
        @if (Model.DefaultView == true)
            {
            @Html.ValidationSummary(true)
            <div class="grid_title">
                <center style="font-size: 11px;color: darkmagenta; border: 1px inset;"><b>New Barno Entry</b></center>
            </div>
            <div class="row">
                <div class="col-lg-3">
                    <div class="editor-label" style="margin-top: 6px;">
                        @Html.Label("", "Bar No.")
                        <b style="color:Red;">*</b>
                    </div>
                </div>
                <div class="col-lg-7">
                    <div class="editor-field">
                        @Html.EditorFor(model => model.NEWBARDATA.BARNO, new { htmlAttributes = new { @class = "form-control", id = "BARNO_NEW", @style = "text-transform:uppercase;", @onkeyup = "toUpper(this);", @placeholder = "Bar No", maxlength = 25 } })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-3">
                    <div class="editor-label" style="margin-top: 6px;">
                        @Html.Label("", "Item")
                        <b style="color:Red;">*</b>
                    </div>
                </div>
                <div class="col-lg-3">
                    <div class="editor-field">
                        @Html.EditorFor(model => model.NEWBARDATA.ITCD, new { htmlAttributes = new { @class = "form-control textbox_image", id = "ITCD_NEW", onkeydown = "GetHelpBlur('" + @Url.Action("GetItemDetails", "Rep_Bar_History") + "','Item Details','ITCD_NEW','ITCD_NEW=itcd=2/ITSTYLE_NEW=ITSTYLE=1')", onblur = "GetHelpBlur('" + @Url.Action("GetItemDetails", "Rep_Bar_History") + "','Item Details','ITCD_NEW','ITCD_NEW=itcd=2/ITSTYLE_NEW=ITSTYLE=1')", @placeholder = "Item Code", maxlength = 10, @title = "Item Code" } })
                        <img src="~/Image/search.png" width="20px" height="20px" class="Help_image_button" title="Help" onmousedown="GetHelpBlur('@Url.Action("GetItemDetails", "Rep_Bar_History")','Item Details','ITCD_NEW','ITCD_NEW=itcd=2/ITSTYLE_NEW=ITSTYLE=1')" />
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="editor-field">
                        @Html.EditorFor(model => model.NEWBARDATA.ITSTYLE, new { htmlAttributes = new { @class = "form-control", id = "ITSTYLE_NEW", @title = "Design", @placeholder = "Design", @readonly = "readonly", @TabIndex = -1 } })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-3">
                    <div class="editor-label" style="margin-top: 6px;">
                        @Html.Label("", "Party Design")
                        @*<b style="color:Red;">*</b>*@
                    </div>
                </div>
                <div class="col-lg-7">
                    <div class="editor-field">
                        @Html.EditorFor(model => model.NEWBARDATA.PDESIGN, new { htmlAttributes = new { @class = "form-control", id = "PDESIGN_NEW", @placeholder = "Party Design", maxlength = 30 } })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-3">
                    <div class="editor-label" style="margin-top: 6px;">
                        @Html.Label("", "Effective Date")
                        @*<b style="color:Red;">*</b>*@
                    </div>
                </div>
                <div class="col-md-9">
                    <div class="editor-field">
                        @Html.EditorFor(model => model.NEWBARDATA.EFFDT, new { htmlAttributes = new { @class = "form-control", id = "EFFDT_NEW", onblur = "DocumentDateCHK(this);", autocomplete = "off", @placeholder = "dd/mm/yyyy", maxlength = 10 } })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-3">
                    <div class="editor-label" style="margin-top: 6px;">
                        @Html.Label("", "CP")
                        @*<b style="color:Red;">*</b>*@
                    </div>
                </div>
                <div class="col-md-9">
                    <div class="editor-field">
                        @Html.EditorFor(model => model.NEWBARDATA.CPRATE, new { htmlAttributes = new { @class = "form-control", id = "CPRATE_NEW", @onkeypress = "return numericOnly(this,2);", maxlength = 14, @title = "CP", @placeholder = "CP" } })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-3">
                    <div class="editor-label" style="margin-top: 6px;">
                        @Html.Label("", "WP")
                        @*<b style="color:Red;">*</b>*@
                    </div>
                </div>
                <div class="col-md-9">
                    <div class="editor-field">
                        @Html.EditorFor(model => model.NEWBARDATA.WPRATE, new { htmlAttributes = new { @class = "form-control", id = "WPRATE_NEW", @onkeypress = "return numericOnly(this,2);", maxlength = 14, @title = "WP", @placeholder = "WP" } })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-3">
                    <div class="editor-label" style="margin-top: 6px;">
                        @Html.Label("", "RP")
                        @*<b style="color:Red;">*</b>*@
                    </div>
                </div>
                <div class="col-md-9">
                    <div class="editor-field">
                        @Html.EditorFor(model => model.NEWBARDATA.RPRATE, new { htmlAttributes = new { @class = "form-control", id = "RPRATE_NEW", @onkeypress = "return numericOnly(this,2);", maxlength = 14, @title = "RP", @placeholder = "RP" } })
                    </div>
                </div>
            </div>
        }
    </div>
    <div class="row">
        <div class="col-lg-3">
        </div>
        <div class="col-lg-3" style="width:auto">
            <div class="text-center"><input type="button" class="btn btn-success" id="btnSaveBar" value="Save" onclick="SaveBarno();" /></div>
        </div>
        <div class="col-lg-3" style="width:auto">
            <div class="text-center"><input type="button" class="btn btn-danger" id="btnSaveBar" value="Cancel" onclick="BarcodeClose();" /></div>
        </div>
    </div>
</div>
