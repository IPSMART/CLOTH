                    <input type="text" id="AddRowsQTY" name="AddRowsQTY" placeholder="rows" style="font-size: 11px;font-weight: bold;width:40px;height:17px;float:left;padding: 2px;" maxlength="3" onkeypress="return numericOnly(this);" />
                    <img src="~/Image/Plus.png" id="btnAdd" title="Add Row" class="Help_image_buttonAddRemove" onclick="AddrowQTY('AddRowsQTY')" />
                    <img src="~/Image/Minus.png" id="btnDelete" title="Delete Row" class="Help_image_buttonAddRemove" onclick="DeleteRowQTY()" />
                }
                <div class="col-md-8">
                    <input type="text" name="search" class="grid_title_box textbox_image" value="" placeholder="Filter" onkeyup="filterTable(event, '__T_OUTISSPROCESS_QtyRequirement_GRID');" />
                    <img src="~/Image/search.png" title="Help" />
                </div>
                @if (Model.DefaultAction != "V")
                {
