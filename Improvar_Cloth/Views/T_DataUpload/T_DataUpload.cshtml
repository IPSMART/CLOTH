@model Improvar.ViewModels.CommonReport
@{
    ViewBag.Title = "Data Upload";
    Layout = "~/Views/Shared/_ReportLayout.cshtml";
}
<script src="~/Scripts/jquery.min.js"></script>

@using (Html.BeginForm())//"T_POS_Upload", "T_POS_Upload", FormMethod.Post, new { enctype = "multipart/form-data" }
{
    <div class="form-horizontal" style="padding-left:5px;padding-right:5px;">
        <div class="row">
            <div class="col-md-3">
                <div class="editor-field" style="margin-top: 4px;">
                    @Html.Label("", "Select File")
                    <b style="color:red"><sup>*</sup></b>
                </div>
            </div>
            <div class="col-md-10">
                <div class="editor-field" style="margin-top: 4px;">
                    <input type="file" name="UploadedFile" required />
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-3">
                <div class="editor-field">

                </div>
            </div>
            <div class="col-md-2">
                <div class="editor-field">
                    <input type="submit" value="Upload" name="Command" class="btn btn-primary" style="margin-top: 4px;padding: 2px 7px 2px 5px;" />
                </div>
            </div>
        </div>
    </div>
    <div class="container">
        <div id="helpDIV">

        </div>
    </div>
}