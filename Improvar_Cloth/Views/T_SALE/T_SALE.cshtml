@model Improvar.ViewModels.TransactionSaleEntry
@{
    string PageControllerName = "T_SALE";
    ViewBag.Title = "Packing Slip Entry";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Partycaption = "Party";

    string COMMONUNIQBAR = "", MNTNCOLOR = "";
    if (Model != null && Model.M_SYSCNFG != null)
    {
        COMMONUNIQBAR = Model.M_SYSCNFG.COMMONUNIQBAR;
        MNTNCOLOR = Model.M_SYSCNFG.MNTNCOLOR;
    }
    int doccdcnt = 0;
    if (Model != null && Model.DocumentType != null)
    {
        doccdcnt = Model.DocumentType.Count;
    }
}
@{
    switch (Model.MENU_PARA)
    {
        case "SBPCK":
            ViewBag.Title = "Packing Slip"; ViewBag.Partycaption = "Buyer"; break;
        case "SB":
            ViewBag.Title = "Sales Bill (Agst Packing Slip)"; ViewBag.Partycaption = "Buyer"; break;
        case "SBDIR":
            ViewBag.Title = "Sales Bill"; ViewBag.Partycaption = "Buyer"; break;
        case "SR":
            ViewBag.Title = "Sales Return (SRM)"; ViewBag.Partycaption = "Buyer"; break;
        case "SBCM":
            ViewBag.Title = "Cash Memo"; ViewBag.Partycaption = "Buyer"; break;
        case "SBCMR":
            ViewBag.Title = "Cash Memo Return Note"; ViewBag.Partycaption = "Buyer"; break;
        case "SBEXP":
            ViewBag.Title = "Sales Bill (Export)"; ViewBag.Partycaption = "Buyer"; break;
        case "PI":
            ViewBag.Title = "Proforma Invoice"; ViewBag.Partycaption = "Party"; break;
        case "PB":
            ViewBag.Title = "Purchase Bill"; ViewBag.Partycaption = "Supplier"; break;
        case "PR":
            ViewBag.Title = "Purchase Return (PRM)"; ViewBag.Partycaption = "Supplier"; break;
        case "OP":
            ViewBag.Title = "Opening Stock"; ViewBag.Partycaption = "Party"; break;
        case "SCN":
            ViewBag.Title = "Sales C/N (w/o Qty)"; ViewBag.Partycaption = "Buyer"; break;
        case "SDN":
            ViewBag.Title = "Sales D/N (w/o Qty)"; ViewBag.Partycaption = "Buyer"; break;
        case "PCN":
            ViewBag.Title = "Purchase C/N (w/o Qty)"; ViewBag.Partycaption = "Supplier"; break;
        case "PDN":
            ViewBag.Title = "Purchase D/N (w/o Qty)"; ViewBag.Partycaption = "Supplier"; break;
        case "OTH":
            ViewBag.Title = "Other Product Opening Stock"; ViewBag.Partycaption = "Party"; break;
        case "PJRC":
            ViewBag.Title = "Receive from Party for Job"; ViewBag.Partycaption = "Party"; break;
        case "PJIS":
            ViewBag.Title = "Issue to Party after Job"; ViewBag.Partycaption = "Party"; break;
        case "PJRT":
            ViewBag.Title = "Return to Party w/o Job"; ViewBag.Partycaption = "Party"; break;
        case "PJBL":
            ViewBag.Title = "Job Bill raised to Party"; ViewBag.Partycaption = "Party"; break;
        case "SBPOS":
            ViewBag.Title = "Cash Sales"; ViewBag.Partycaption = "Buyer"; break;
        default: ViewBag.Title = ""; ViewBag.Partycaption = "Party"; break;
    }
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .TextAreaRemarks {
        height: 190px;
    }
</style>
<link rel="stylesheet" href="~/Content/bootstrap-float-label.min.css" />
<script src="~/Scripts/jquery.min.js"></script>
<script src="~/Scripts/Cloth.js?Ver=@System.DateTime.Now"></script>
<script src="~/Scripts/Sale.js?Ver=@System.DateTime.Now"></script>
<script type="text/javascript">
    $("#WaitingMode").show();
    $(document).ready(function () {
        if ("@Model.SHOWMTRLJOBCD" != "Y") {
            $("#divmtrljobcd").hide();
        }
        if ("@Model.SHOWBLTYPE" != "Y") {
            $(".divbltype").hide();
        }
        if ("@Model.SHOWSTKTYPE" != "Y") {
            $(".divstktype").hide();
        }
        if (("@Model.MENU_PARA" == "PB" || "@Model.MENU_PARA" == "PR") && "@COMMONUNIQBAR" == "E" && "@Model.DefaultAction" != "V") {
            $(".DivMergeBarItem").show();
        }
        else {
            $(".DivMergeBarItem").hide();
        }
        if ("@Model.DefaultAction" == "V" || "@Model.DefaultAction" == "D") {
            modeView();
            $("#update_page_slno").hide();
            $("#cancel_page_slno").hide();

        }
        else if ("@Model.DefaultAction" == "E") {
            var BARGENTYPE = $("#BARGENTYPE").val();
            if (BARGENTYPE == "E") {
                $("#BARGENTYPE").attr("style", "pointer-events:none");
            }
            if ('@Model.MENU_PARA' == "SB") {
                $('#PACKSLIP_HELP').hide("");
                $("#LINKDOCNO").attr("readonly", "readonly");
            }
            else {
                GetAllMtrljobcd();
            }
            Checked_Disable();
            if ('@Model.MENU_PARA' == "PB" || '@Model.MENU_PARA' == "PR") {
                changeBARGENTYPE();
            }
            @*if ('@Model.MENU_PARA' != "PB" && '@Model.MENU_PARA' != "SB" && '@Model.MENU_PARA' != "OP") {*@
            if ('@Model.MENU_PARA' != "SB") {
                $('.barfirstrow input').attr('readonly', 'readonly');
                $('.barfirstrow input').removeAttr("onblur");
                $('.barfirstrow').find(".Help_image_button").hide();
                $('.barfirstrow input').removeAttr("onkeydown");
                $('.barfirstrow input').attr('TabIndex', '-1');
            }
            if ('@Model.MENU_PARA' != "SBPCK" && '@Model.MENU_PARA' != "SB" && '@Model.MENU_PARA' != "SBDIR" && '@Model.MENU_PARA' != "SR" && '@Model.MENU_PARA' != "SBEXP" && '@Model.MENU_PARA' != "SBPOS") {
                $(".fabitcdhlpdiv").show();
                $(".fabitcddiv").hide();
                if ('@MNTNCOLOR' == "Y") {
                    $(".colrcdhlpdiv").show();
                    $(".colrcddiv").hide();
                }
            }
            else {
                $(".fabitcdhlpdiv").hide();
                $(".fabitcddiv").show();
                if ('@MNTNCOLOR' == "Y") {
                    $(".colrcdhlpdiv").hide();
                    $(".colrcddiv").show();
                }
            }
            if ('@Model.MENU_PARA' == "PJBL") {
                $('.PJBLDiv input').attr('readonly', 'readonly');
                $('.PJBLDiv input').removeAttr("onblur");
                $('.PJBLDiv input').attr('TabIndex', '-1');
                $('.PJBLDiv').find(".Help_image_button_grid").hide();
                $('.PJBLDiv input').removeAttr("onkeydown");
            }
        }
        else if ("@Model.DefaultAction" == "A") {
            $('form :input:enabled:visible:not([readonly]):first').focus();
            if ('@Model.MENU_PARA' != "SB") {
                GetAllMtrljobcd();
            }
            @*if ('@Model.MENU_PARA' != "PB" && '@Model.MENU_PARA' != "SB" && '@Model.MENU_PARA' != "OP") {*@
            if ('@Model.MENU_PARA' != "SB") {
                $('.barfirstrow input').attr('readonly', 'readonly');
                $('.barfirstrow input').removeAttr("onblur");
                $('.barfirstrow').find(".Help_image_button").hide();
                $('.barfirstrow input').removeAttr("onkeydown");
                $('.barfirstrow input').attr('TabIndex', '-1');
            }
            if ('@Model.MENU_PARA' != "SBPCK" && '@Model.MENU_PARA' != "SB" && '@Model.MENU_PARA' != "SBDIR" && '@Model.MENU_PARA' != "SR" && '@Model.MENU_PARA' != "SBEXP" && '@Model.MENU_PARA' != "SBPOS") {
                $(".fabitcdhlpdiv").show();
                $(".fabitcddiv").hide();
                if ('@MNTNCOLOR' == "Y") {
                    $(".colrcdhlpdiv").show();
                    $(".colrcddiv").hide();
                }
            }
            else {
                $(".fabitcdhlpdiv").hide();
                $(".fabitcddiv").show();
                if ('@MNTNCOLOR' == "Y") {
                    $(".colrcdhlpdiv").hide();
                    $(".colrcddiv").show();
                }
            }
        }
        if ("@Model.LINKDOCNO" != "" && '@Model.MENU_PARA' == "SB") {
            $('#_T_SALE_DETAIL_GRID input').attr('readonly', 'readonly');
            $('#_T_SALE_PRODUCT_GRID input').attr('readonly', 'readonly');
            $('#_T_SALE_DETAIL_GRID').find(".Help_image_button_grid").hide();
            $('#_T_SALE_DETAIL_GRID input').removeAttr("onblur");
            $('#_T_SALE_PRODUCT_GRID input').attr('TabIndex', '-1');
            $('#_T_SALE_DETAIL_GRID input').attr('TabIndex', '-1');
        }
        if ("@Model.DefaultAction" == "A" || "@Model.DefaultAction" == "E") {
            if ("@Model.MENU_PARA" == "SBPCK" || "@Model.MENU_PARA" == "SB" || "@Model.MENU_PARA" == "SBDIR" || "@Model.MENU_PARA" == "SR" || "@Model.MENU_PARA" == "SBEXP" || "@Model.MENU_PARA" == "PI" || "@Model.MENU_PARA" == "SBPOS") {
                var CONVQTYPUNIT = retFloat($("#CONVQTYPUNIT").val()).toFixed(0);
                if (CONVQTYPUNIT == "0") {
                    $(".Billuomdiv").hide();
                }
            }
        }

        $("#WaitingMode").hide();
    });
</script>
<script>
    function requiredFieldValidator() {
        debugger;
        if ("@doccdcnt" == 0) {
            msgInfo("Select Document Code !!");
            message_value = "DOCCD";
            return false;
        }
        if (!emptyFieldCheck("Please Select / Enter Document Date", "DOCDT")) { return false; }
        if (!emptyFieldCheck("Please Select / Enter @ViewBag.Partycaption", "SLCD")) { return false; }
        if ("@Model.MENU_PARA" == "PJBL") {
            if (!emptyFieldCheck("Please Select / Enter Job", "JOBCD")) { return false; }
        }
        if (!emptyFieldCheck("Please Select / Enter Godown", "GOCD")) {
            $("li").removeClass("active").addClass("");
            $(".nav-tabs li:first-child").addClass('active');
            $(".tab-content div").removeClass("active");
            $(".tab-content div:first-child").removeClass("tab-pane fade").addClass("tab-pane fade in active");
            return false;
        }
        if ("@Model.MENU_PARA" == "PB") {
            if (!emptyFieldCheck("Please Enter Party bill no", "PREFNO")) {
                $("li").removeClass("active").addClass("");
                $(".nav-tabs li:first-child").addClass('active');
                $(".tab-content div").removeClass("active");
                $(".tab-content div:first-child").removeClass("tab-pane fade").addClass("tab-pane fade in active");
                return false;
            }
            if (!emptyFieldCheck("Please Enter Party bill date", "PREFDT")) {
                $("li").removeClass("active").addClass("");
                $(".nav-tabs li:first-child").addClass('active');
                $(".tab-content div").removeClass("active");
                $(".tab-content div:first-child").removeClass("tab-pane fade").addClass("tab-pane fade in active");
                return false;
            }
        }
        if ("@Model.MENU_PARA" == "SCN" || "@Model.MENU_PARA" == "SDN" || "@Model.MENU_PARA" == "PCN" || "@Model.MENU_PARA" == "PDN") {
            if (!emptyFieldCheck("Please Enter Exp. Gen.Ledger", "EXPGLCD")) {
                $("li").removeClass("active").addClass("");
                $(".nav-tabs li:first-child").addClass('active');
                $(".tab-content div").removeClass("active");
                $(".tab-content div:first-child").removeClass("tab-pane fade").addClass("tab-pane fade in active");
                return false;
            }
        }
        @*if ("@Model.MENU_PARA" == "PB" || "@Model.MENU_PARA" == "OP" || "@Model.MENU_PARA" == "OTH") {*@
        var GridRow = $("#_T_SALE_DETAIL_GRID > tbody > tr").length;
        var balenoexist = false;
        if (GridRow != 0) {
            for (var i = 0; i <= GridRow - 1; i++) {
                if ($("#D_BALENO_" + i).val() != "") {
                    balenoexist = true;
                    break;
                }
            }
        }
        if (balenoexist == true && $("#LRNO").val() == "" && "@Model.MENU_PARA" != "SBPCK" && "@Model.MENU_PARA" != "SB" && "@Model.MENU_PARA" != "SBDIR" && "@Model.MENU_PARA" != "SR" && "@Model.MENU_PARA" != "SBEXP" && "@Model.MENU_PARA" != "SBPOS") {
            msgInfo("if Bale No. exist then you have to fill LR Number in Transport Tab !!");
            $("li").removeClass("active").addClass("");
            $(".nav-tabs li:nth-child(5)").addClass('active');
            //below set the  child sequence
            $(".tab-content div").removeClass("active");
            $(".tab-content div:nth-child(5)").removeClass("tab-pane fade").addClass("tab-pane fade in active");
            message_value = "LRNO";
            return false;
        }
        //}
        //AMT TAB HSN CHECK
        if ("@Model.MENU_PARA" == "SCN" || "@Model.MENU_PARA" == "SDN" || "@Model.MENU_PARA" == "PCN" || "@Model.MENU_PARA" == "PDN") {
            var GridRowAmt = $("#AMOUNT_GRID > tbody > tr").length;
            if (GridRowAmt != 0) {
                for (var i = 0; i <= GridRowAmt - 1; i++) {
                    if ($("#HSNCODE_" + i).val() == "" && retFloat($("#A_AMT_" + i).val()) != 0) {
                        msgInfo("Please enter HSN Code at Sl No. (" + $("#ASLNO_" + i).val() + ")  in Amount Tab!!");
                        $("li").removeClass("active").addClass("");
                        $(".nav-tabs li:nth-child(4)").addClass('active');
                        //below set the  child sequence
                        $(".tab-content div").removeClass("active");
                        $(".tab-content div:nth-child(4)").removeClass("tab-pane fade").addClass("tab-pane fade in active");
                        message_value = "HSNCODE_" + i;
                        return false;
                    }
                }
            }
        }

        //
        var GridRow = $("#_T_SALE_PRODUCT_GRID > tbody > tr").length;
        var rateblank = false;
        if (GridRow != 0) {
            for (var i = 0; i <= GridRow - 1; i++) {
                if ($("#B_ITCD_" + i).val() != "" && retFloat($("#B_RATE_" + i).val()) == 0) {
                    rateblank = true;
                    break;
                }
            }
            if (rateblank == true) {
                var slno = $("#B_SLNO_" + i).val();
                msgInfo("Please enter rate at Sl No. (" + slno + ")  in Bar Code Tab!!");
                $("li").removeClass("active").addClass("");
                $(".nav-tabs li:nth-child(2)").addClass('active');
                //below set the  child sequence
                $(".tab-content div").removeClass("active");
                $(".tab-content div:nth-child(2)").removeClass("tab-pane fade").addClass("tab-pane fade in active");
                message_value = "B_RATE_" + i;
                return false;
            }
        }
        //start of finalise
        if ($("#bardatachng").val() == "Y" && GridRow > 0) {
            debugger;
            var GridRow = $("#_T_SALE_PRODUCT_GRID > tbody > tr").length;
            var mtrlcdblank = false; var slnoblank = false; var glcdblank = false; var qntyblank = false;
            if (GridRow != 0) {
                for (var i = 0; i <= GridRow - 1; i++) {
                    if ($("#B_MTRLJOBCD_" + i).val() == "") {
                        mtrlcdblank = true;
                        break;
                    }
                    if ($("#B_TXNSLNO_" + i).val() == "") {
                        slnoblank = true;
                        break;
                    }
                    if ($("#B_GLCD_" + i).val() == "") {
                        glcdblank = true;
                        break;
                    }
                    if (retFloat($("#B_QNTY_" + i).val()) == 0) {
                        qntyblank = true;
                        break;
                    }
                }
            }
            if (mtrlcdblank == true) {
                msgInfo("Please Fill Material Job in Barcode Grid !!");
                $("li").removeClass("active").addClass("");
                $(".nav-tabs li:nth-child(2)").addClass('active');
                //below set the  child sequence
                $(".tab-content div").removeClass("active");
                $(".tab-content div:nth-child(2)").removeClass("tab-pane fade").addClass("tab-pane fade in active");
                message_value = "B_MTRLJOBCD_" + i;
                return false;
            }
            if (slnoblank == true) {
                msgInfo("Please Fill Bill Sl # in Barcode Grid !!");
                $("li").removeClass("active").addClass("");
                $(".nav-tabs li:nth-child(2)").addClass('active');
                //below set the  child sequence
                $(".tab-content div").removeClass("active");
                $(".tab-content div:nth-child(2)").removeClass("tab-pane fade").addClass("tab-pane fade in active");
                message_value = "B_TXNSLNO_" + i;
                return false;
            }
            if (glcdblank == true) {
                var slno = $("#B_SLNO_" + i).val();
                msgInfo("Glcd blank in Barcode Grid at slno " + slno + " !!Please add glcd in Group Master");
                $("li").removeClass("active").addClass("");
                $(".nav-tabs li:nth-child(2)").addClass('active');
                //below set the  child sequence
                $(".tab-content div").removeClass("active");
                $(".tab-content div:nth-child(2)").removeClass("tab-pane fade").addClass("tab-pane fade in active");
                message_value = "B_TXNSLNO_" + i;
                return false;
            }
            if (qntyblank == true && "@Model.MENU_PARA" != "SCN" && "@Model.MENU_PARA" != "SDN" && "@Model.MENU_PARA" != "PCN" && "@Model.MENU_PARA" != "PDN") {
                var slno = $("#B_SLNO_" + i).val();
                msgInfo("Please Fill Quantity in Barcode Grid at slno " + slno + " !!");
                $("li").removeClass("active").addClass("");
                $(".nav-tabs li:nth-child(2)").addClass('active');
                //below set the  child sequence
                $(".tab-content div").removeClass("active");
                $(".tab-content div:nth-child(2)").removeClass("tab-pane fade").addClass("tab-pane fade in active");
                message_value = "B_QNTY_" + i;
                return false;
            }
            $.ajax({
                type: 'POST',
                beforesend: $("#WaitingMode").show(),
                url: $("#UrlFillDetailData").val(),
                data: $('form').serialize(),
                success: function (result) {
                    if (result.indexOf("_T_SALE_DETAIL_GRID") == -1) {
                        msgInfo("Bill Sl (" + result + ") duplicate in barcode tab!");
                        $("#WaitingMode").hide();
                        $("li").removeClass("active").addClass("");
                        $(".nav-tabs li:nth-child(2)").addClass('active');
                        //below set the  child sequence
                        $(".tab-content div").removeClass("active");
                        $(".tab-content div:nth-child(2)").removeClass("tab-pane fade").addClass("tab-pane fade in active");
                        message_value = "FillDetail";
                        return false;
                    }
                    else {
                        $("#partialdivDetail").html(result);
                        var GridRow = $("#_T_SALE_DETAIL_GRID > tbody > tr").length;
                        for (var i = 0; i <= GridRow - 1; i++) {
                            CalculateAmt_Details(i);
                        }
                        $("#bardatachng").val("N");
                    }
                    debugger;
                    pageValidation("@Model.DefaultAction");

                },
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    $("#WaitingMode").hide();
                    msgError(XMLHttpRequest.responseText);
                    $("body span h1").remove(); $("#msgbody_error style").remove();
                }
            });
            return false;
        }
            //start of finalise
        else {
            return true;
        }
    }
    document.onkeyup = ClosePendOrder;
</script>
