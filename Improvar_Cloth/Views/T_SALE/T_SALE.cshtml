            url = updateQueryStringParameter(url, "loadOrder", "Y");
            window.location = url;
        }
        function ReloadWithPSLIP() {
            var searchValue = $('#LINKAUTONO').val();
            var searchValue1 = $('#LINKDOCNO').val();
