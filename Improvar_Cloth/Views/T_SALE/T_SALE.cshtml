            }
        }

    });
</script>
<script>
    function pageprinting() {
        debugger;
        var ty = $("#DOCCD").val();
        $.ajax({
            type: 'POST',
            url: "@Url.Action("Print", PageControllerName)",
            data: $('form').serialize() + "&DOCNO=" + $("#DOCNO").val() + "&DOC_CD=" + $("#DOCCD").val() + "&DOCDT=" + $("#DOCDT").val(),
            success: function (result) {
                if ('@Model.MENU_PARA' == "PJIS" || '@Model.MENU_PARA' == "PJRC" || '@Model.MENU_PARA' == "PJRT") {
                    window.open("@Url.Action("Rep_Doc_Print", "Rep_Doc_Print", new { US = Model.UNQSNO_ENCRYPTED })", "_blank")
                }
                else {
                    window.open("@Url.Action("SaleBill_Print", "SaleBill_Print", new { US = Model.UNQSNO_ENCRYPTED })", "_blank")
                }

            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                $("#WaitingMode").hide();
                msgError(XMLHttpRequest.responseText);
                $("body span h1").remove(); $("#msgbody_error style").remove();
            }
        });
    }
    function Barcodeprinting() {
        debugger;
        var AUTONO = $("#AUTONO").val();
        var DOCDT = $("#DOCDT").val();
        window.open("@Url.Action("Rep_BarcodePrint", "Rep_BarcodePrint")?US=" + '@Model.UNQSNO_ENCRYPTED' + "&autono=" + AUTONO + "&docdt=" + DOCDT, "_blank")
    }
</script>
<script>
    //focuc on first element of tab
    $(document).keyup(function (event) {
        debugger;
        var KeyID = (window.event) ? event.keyCode : e.keyCode;
        if (event.altKey && KeyID == 77) {//for Alt+M (main code tab)
            $("li").removeClass("active").addClass("");
            $(".nav-tabs li:first-child").addClass('active');
            $(".tab-content div").removeClass("active");
            $(".tab-content div:first-child").removeClass("tab-pane fade").addClass("tab-pane fade in active");
            if ("@Model.MENU_PARA" == "PB" || "@Model.MENU_PARA" == "OP" || "@Model.MENU_PARA" == "OTH" || "@Model.MENU_PARA" == "PJRC") {
                $("#PREFNO").focus();
            }
            else {
                $("#CONSLCD").focus();
            }
        }
        if (event.altKey && KeyID == 66) {//for Alt+B (bar code tab)
            $("li").removeClass("active").addClass("");
            $(".nav-tabs li:nth-child(2)").addClass('active');
            //below set the  child sequence
            $(".tab-content div").removeClass("active");
            $(".tab-content div:nth-child(2)").removeClass("tab-pane fade").addClass("tab-pane fade in active");
            if ($("#MNTNBARNO").val() == "Y") {
                $("#BARCODE").focus();
            }
            else {
                $("#STYLENO").focus();
            }
        }
        if (event.altKey && KeyID == 84 && "@Model.MENU_PARA" != "SR" && "@Model.MENU_PARA" != "PR") {//for Alt+T (transport tab)
            $("li").removeClass("active").addClass("");
            $(".nav-tabs li:nth-child(5)").addClass('active');
            //below set the  child sequence
            $(".tab-content div").removeClass("active");
            $(".tab-content div:nth-child(5)").removeClass("tab-pane fade").addClass("tab-pane fade in active");
            $("#TRANSLCD").focus();
        }
        if (event.altKey && KeyID == 72) {//for Alt+H (amount tab)
