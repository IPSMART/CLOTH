@model Improvar.ViewModels.SysCnfgMasterEntry
@{
    ViewBag.Title = "Posting & Terms Setup";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@{
    string PageControllerName = "M_SysCnfg";
}

<script src="~/Scripts/jquery.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        if ("@Model.DefaultAction" == "V" || "@Model.DefaultAction" == "D") {
            modeView();
        }
        else if ("@Model.DefaultAction" == "E") {

            $("#EFFDT").attr("readonly", "readonly");
        }
        else if ("@Model.DefaultAction" == "A") {
          
            $('form :input:enabled:visible:not([readonly]):first').focus();
            Check_SysCnfg_DataExsistOrNot();
        }
        $("#WaitingMode").hide();
    });
</script>
<script>
    function Check_SysCnfg_DataExsistOrNot()
    {
        debugger;
        var chkdata = $('#Checked_DataExsist').val();
        if (chkdata == "True") {
            $("#EFFDT").attr("readonly", "readonly");
            if ($("#SALDEBGLCD").val() != "")
            { $("#SALDEBGLCD").attr("readonly", "readonly"); $("#help_SALDEBGLCD").hide(); }
            if ($("#PURDEBGLCD").val() != "")
            { $("#PURDEBGLCD").attr("readonly", "readonly"); $("#help_PURDEBGLCD").hide(); }
            if ($("#CLASS1CD").val() != "")
            { $("#CLASS1CD").attr("readonly", "readonly"); $("#help_CLASS1CD").hide(); }
            if ($("#RETDEBSLCD").val() != "")
            { $("#RETDEBSLCD").attr("readonly", "readonly"); $("#PARTY_HELP").hide(); }
            var WPPRICEGENCD = $("#WPPRICEGENCD").val();
            if (WPPRICEGENCD != "") $("#WPPRICEGENCD").attr("style", "pointer-events:none");
            if ($("#WPPRICEGENAMT").val() != "") $("#WPPRICEGENAMT").attr("readonly", "readonly");
            var RPPRICEGENCD = $("#RPPRICEGENCD").val();
            if (RPPRICEGENCD != "") $("#RPPRICEGENCD").attr("style", "pointer-events:none");
            if ($("#RPPRICEGENAMT").val() != "") $("#RPPRICEGENAMT").attr("readonly", "readonly");
            //var DUEDATECALCON = ($("#DUEDATECALCON").find(":selected").val());
            var DUEDATECALCON = $("#DUEDATECALCON").val();
            if (DUEDATECALCON != "") $("#DUEDATECALCON").attr("style", "pointer-events:none");
            if ($("#WPPER").val() != "") $("#WPPER").attr("readonly", "readonly");
            if ($("#RPPER").val() != "") $("#RPPER").attr("readonly", "readonly");
            if ($("#PRICEINCODE").val() != "") $("#PRICEINCODE").attr("readonly", "readonly");
            if ($("#PRICEINCODECOST").val() != "") $("#PRICEINCODECOST").attr("readonly", "readonly");
            if ($("#RTDEBCD").val() != "") { $("#RTDEBCD").attr("readonly", "readonly"); $("#help_RTDEBCD").hide(); }
            if ($("#DESIGNPATH").val() != "") $("#DESIGNPATH").attr("readonly", "readonly");
        }
       }
</script>
<script>
    function requiredFieldValidator() {
        if (!emptyFieldCheck("Select Effective Date ", "EFFDT")) { return false; }
        if (!emptyFieldCheck("Select Debtors ", "SALDEBGLCD")) { return false; }
        if (!emptyFieldCheck("Select Creditors ", "PURDEBGLCD")) { return false; }
        return true;
    }
</script>
<script>
    if ("@Model.DefaultAction" == "A") {
        debugger;
       
            $(function () {
                var chkdata = $('#Checked_DataExsist').val();
                if (chkdata != "True")
                { $("#EFFDT").datepicker({ dateFormat: "dd/mm/yy", changeMonth: true, changeYear: true }); }
                else { return false;}
               
            });
        
    
    }
</script>
<script>
