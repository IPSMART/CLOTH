@model Improvar.ViewModels.SysCnfgMasterEntry
@{
    ViewBag.Title = "Posting & Terms Setup";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@{
    string PageControllerName = "M_SysCnfg";
}

<script src="~/Scripts/jquery.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        if ("@Model.DefaultAction" == "V" || "@Model.DefaultAction" == "D") {
            modeView();
        }
        else if ("@Model.DefaultAction" == "E") {

            $("#EFFDT").attr("readonly", "readonly");
        }
        else if ("@Model.DefaultAction" == "A") {
            $('form :input:enabled:visible:not([readonly]):first').focus();
        }
        $("#WaitingMode").hide();
    });
</script>
<script>
    function requiredFieldValidator() {
        if (!emptyFieldCheck("Select Effective Date ", "EFFDT")) { return false; }
        if (!emptyFieldCheck("Select Debtors ", "SALDEBGLCD")) { return false; }
        if (!emptyFieldCheck("Select Creditors ", "PURDEBGLCD")) { return false; }
        return true;
    }
</script>
<script>
    if ("@Model.DefaultAction" == "A") {
        $(function () { $("#EFFDT").datepicker({ dateFormat: "dd/mm/yy", changeMonth: true, changeYear: true }); });
    }
</script>
<script>
    @{
        string mldauto = "";
        if (@Model.M_SYSCNFG != null)
        {
            mldauto = Model.M_SYSCNFG.M_AUTONO.ToString();
        }
        else
        {
            mldauto = "";
        }
    }
    function ModifyLogDetails() {
        if ("@Model.DefaultAction" == "V") {
            ModifyLogDtl(@mldauto);
        }
    }
</script>

<script>
    function ReloadWithData() {
        var searchValue = $('#SEARCH_AUTONO').val();
        var url = window.location.href;
        var key1 = "searchValue";
        url = updateQueryStringParameter(url, key1, searchValue);
        url = updateQueryStringParameter(url, "loadItem", "Y");
        window.location = url;
    }
</script>
<script>
    function ChkPriceInCode() {
        debugger;
        var FstPricecode = $('#PRICEINCODE').val();
        if (FstPricecode == "") return;
        if (FstPricecode.length < 10) {
            msgWarning('First ten character is mandatory !');
            $('#PRICEINCODE').val("");
            message_value = "PRICEINCODE";
            return false;
        }
        var all = "ABCDEFGHIJKLMNOPQRSTUVWXYZ";
        for (var i = 0; i < all.length; i++) {
            var char = all.charAt(i);
            if (FstPricecode.split(char).length > 2) {
                msgWarning('We cant use duplicate charecter !');
                $('#PRICEINCODE').val("");
                message_value = "PRICEINCODE";
                return true;
            }
        }
    }
</script>

