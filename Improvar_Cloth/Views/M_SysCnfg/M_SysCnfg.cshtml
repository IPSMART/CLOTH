<script src="~/Scripts/jquery.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        if ("@Model.DefaultAction" == "V" || "@Model.DefaultAction" == "D") {
            modeView();
            $("#PRICEINCODERATE").attr("readonly", false);
            $("#PRICEINCODECOSTRATE").attr("readonly", false);
        }
        else if ("@Model.DefaultAction" == "E") {

            $("#EFFDT").attr("readonly", "readonly");
            var chkdata = $('#Checked_DataExsist').val();
            if (chkdata == "True") {
                if ($("#SALDEBGLCD").val() != "")
                { $("#SALDEBGLCD").attr("readonly", "readonly"); $("#help_SALDEBGLCD").hide(); }
                if ($("#PURDEBGLCD").val() != "")
                { $("#PURDEBGLCD").attr("readonly", "readonly"); $("#help_PURDEBGLCD").hide(); }
                if ($("#CLASS1CD").val() != "")
                { $("#CLASS1CD").attr("readonly", "readonly"); $("#help_CLASS1CD").hide(); }
                if ($("#RETDEBSLCD").val() != "")
                { $("#RETDEBSLCD").attr("readonly", "readonly"); $("#PARTY_HELP").hide(); }
                var WPPRICEGENCD = $("#WPPRICEGENCD").val();
                var RPPRICEGENCD = $("#RPPRICEGENCD").val();
                var DUEDATECALCON = $("#DUEDATECALCON").val();
                if ($("#RTDEBCD").val() != "") { $("#RTDEBCD").attr("readonly", "readonly"); $("#help_RTDEBCD").hide(); }
                if ($("#DESIGNPATH").val() != "") $("#DESIGNPATH").attr("readonly", "readonly");
            }
        }
        else if ("@Model.DefaultAction" == "A") {

            $('form :input:enabled:visible:not([readonly]):first').focus();
            Check_SysCnfg_DataExsistOrNot();
        }
        $("#WaitingMode").hide();
    });
</script>
<script>
    function Check_SysCnfg_DataExsistOrNot() {
        debugger;
        var chkdata = $('#Checked_DataExsist').val();
        if (chkdata == "True") {
            if ($("#SALDEBGLCD").val() != "")
            { $("#SALDEBGLCD").attr("readonly", "readonly"); $("#help_SALDEBGLCD").hide(); }
            if ($("#PURDEBGLCD").val() != "")
