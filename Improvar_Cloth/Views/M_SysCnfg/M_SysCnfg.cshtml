            mldauto = "";
        }
    }
    function ModifyLogDetails() {
        if ("@Model.DefaultAction" == "V") {
            ModifyLogDtl(@mldauto);
        }
    }
</script>

<script>
    function ReloadWithData() {
        var searchValue = $('#SEARCH_AUTONO').val();
        var url = window.location.href;
        var key1 = "searchValue";
        url = updateQueryStringParameter(url, key1, searchValue);
        url = updateQueryStringParameter(url, "loadItem", "Y");
        window.location = url;
    }
</script>
<script>
    function ChkPriceInCode(tag) {
        debugger;
        var all = "ABCDEFGHIJKLMNOPQRSTUVWXYZ";
        if (tag != "y")
        {
            var FstPricecode = $('#PRICEINCODE').val();
            if (FstPricecode == "") return;
            if (FstPricecode.length < 10) {
                msgWarning('First ten character is mandatory !');
                $('#PRICEINCODE').val("");
                message_value = "PRICEINCODE";
                return false;
            }
          
            for (var i = 0; i < all.length; i++) {
                var char = all.charAt(i);
                if (FstPricecode.split(char).length > 2) {
                    msgWarning('We cant use duplicate charecter !');
                    $('#PRICEINCODE').val("");
                    message_value = "PRICEINCODE";
                    return true;
                }
            }
        }
        else {
            var FstPricecode = $('#PRICEINCODECOST').val();
            if (FstPricecode == "") return;
            if (FstPricecode.length < 11) {
                msgWarning('First eleven character is mandatory !');
                $('#PRICEINCODECOST').val("");
                message_value = "PRICEINCODECOST";
                return false;
            }
            //var all = "ABCDEFGHIJKLMNOPQRSTUVWXYZ";
            for (var i = 0; i < all.length; i++) {
                var char = all.charAt(i);
                if (FstPricecode.split(char).length > 2) {
                    msgWarning('We cant use duplicate charecter !');
                    $('#PRICEINCODECOST').val("");
                    message_value = "PRICEINCODECOST";
                    return true;
                }
            }
        }
      
     
    }
</script>

