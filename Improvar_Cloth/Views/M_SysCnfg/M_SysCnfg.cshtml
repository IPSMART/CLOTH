<script>
    function ChkPriceInCode(tag) {
        debugger;
        var all = "ABCDEFGHIJKLMNOPQRSTUVWXYZ";
        if (tag != "y") {
            var FstPricecode = $('#PRICEINCODE').val();
            if (FstPricecode == "") return;
            if (FstPricecode.length < 10) {
                msgWarning('First ten character is mandatory !');
                $('#PRICEINCODE').val("");
                message_value = "PRICEINCODE";
                return false;
            }

            for (var i = 0; i < all.length; i++) {
                var char = all.charAt(i);
                if (FstPricecode.split(char).length > 2) {
                    msgWarning('We cant use duplicate charecter !');
                    $('#PRICEINCODE').val("");
                    message_value = "PRICEINCODE";
                    return true;
                }
            }
        }
        else {
            var FstPricecode = $('#PRICEINCODECOST').val();
            if (FstPricecode == "") return;
            if (FstPricecode.length < 11) {
                msgWarning('First eleven character is mandatory !');
                $('#PRICEINCODECOST').val("");
                message_value = "PRICEINCODECOST";
                return false;
            }
            //var all = "ABCDEFGHIJKLMNOPQRSTUVWXYZ";
            for (var i = 0; i < all.length; i++) {
                var char = all.charAt(i);
                if (FstPricecode.split(char).length > 2) {
                    msgWarning('We cant use duplicate charecter !');
                    $('#PRICEINCODECOST').val("");
                    message_value = "PRICEINCODECOST";
                    return true;
                }
            }
        }


    }
</script>

<script>
    //Price code rate encode
    function RateEncode(tag) {
        debugger;
        var rate = 0; var priceincode = 0;
        if (tag != "y") {
            rate = $("#PRICEINCODERATE").val();
            priceincode = $("#PRICEINCODE").val();
        }
        else {
            rate = $("#PRICEINCODECOSTRATE").val();
            priceincode = $("#PRICEINCODECOST").val();
        }
        if (rate == "") {
            if (tag != "y")
            { $("#PRICEINCODERATENCOD").val(""); }
            else { $("#PRICEINCODECOSTRATENCOD").val(""); }
        }
        else {
            if (priceincode != "" && rate != "") {
                $.ajax({
                    type: 'post',
                    url: "@Url.Action("RateEncode", PageControllerName )",
                    data: "&rate=" + rate + "&PRICEINCODE=" + priceincode,
                    beforesend: $("#WaitingMode").show(),
                    success: function (result) {
                        $("#WaitingMode").hide();
                        if (result != "") {
                            $("#tempHDD").val(result);
                            var str = $("#tempHDD").val().split(String.fromCharCode(181));
                            if (tag != "y")
                            { $("#PRICEINCODERATENCOD").val(str[0]); }
                            else { $("#PRICEINCODECOSTRATENCOD").val(str[0]); }

                        } else {
                            if (tag != "y")
                            { $("#PRICEINCODERATENCOD").val(""); }
                            else { $("#PRICEINCODECOSTRATENCOD").val(""); }
                        }
                    },
                    error: function (XMLHttpRequest, textStatus, errorThrown) {
                        $("#WaitingMode").hide();
                        msgError("Error: " + textStatus + "," + errorThrown);
                    }
                });
            }
        }

    }
</script>
